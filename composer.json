    "config": {
        "platform": {
            "php": "7.4.20"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "funkjedi/composer-include-files": true
        }
    },
    "extra": {
        "include_files": [
            "app/Helpers/settings.php",
            "_/api/cart.php",
            "_/api/dpo.php",
            "_/api/helpers.php",
            "_/api/image.php",
            "_/api/member.php",
            "_/api/membership.php",
            "_/api/membership_access.php",
            "_/api/metadata.php",
            "_/api/page.php",
            "_/api/product.php",
            "_/api/volt.php"
        ],
        "laravel": {
            "dont-discover": [
                "botman/botman",
                "laravel/telescope",
                "omniphx/forrest"
            ]
        }
    },
    "autoload": {
        "files": [
            "app/Illuminate/Database/DatabaseServiceProvider.php",
            "app/Common/Swift_Validate.php"
        ],
        "psr-4": {
            "Ds\\": "app",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Stories\\": "database/stories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    }
}

{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "editor.defaultFormatter": null,
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "stylelint.config": {},
  "stylelint.configFile": "resources/apps/stylelint.config.js",
  "stylelint.customSyntax": "postcss-scss",
  "stylelint.validate": [
      "css",
      "html",
      "less",
      "markdown",
      "postcss",
      "sass",
      "scss",
      "source.css.styled",
      "source.markdown.math",
      "styled-css",
      "sugarss",
      "svelte",
      "vue",
      "vue-html",
      "vue-postcss",
      "xml",
      "xsl"
    ],
}
